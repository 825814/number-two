<p id="notice"></p>
<div class="container">

  <div class="row">
    <div class="col-md-12">
      <table class="table">

        <% @questions.each do |question| %>
        <tr>
          <td>
          <%= link_to admin_path(question.id) do %>
            <%= question.question %>
          <% end %>
          </td>
          <td><%= link_to "変更する", edit_admin_question_path(question.id) %></td>
          <td><%= link_to "削除する", admin_question_path(question.id), method: :delete %></td>
        </tr>
        <% end %>

