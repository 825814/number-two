<h1>ユーザー側のトップページで表示する</h1>

<%= form_with model: @user, url:'/admin/users', method: :post do |f| %>

  <h4>テーマ</h4>
  <%= f.text_field :title %>

  <h4>説明文</h4>
  <%= f.text_area :body %>
   追加部分 
  <h4>画像</h4>
  <%= f.file_field :image, accept: "image/*" %>

  <%= f.submit '投稿' %>

<% end %>